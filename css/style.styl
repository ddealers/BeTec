              img
                /*display block
                margin 2vh auto 0*/
                display: list-item;
                margin-left: 50px;
            .step-data
              @media screen and (max-width: 640px)
                padding-left 1em
              input
                display block
                padding 0.8vw
                margin 0.5vw 0
                border 2px #ececec solid
                width 90%
                font-size: 11px;
              input[type="checkbox"]
                position: absolute;
                right: 400px;
                bottom: 60px;
              label
                position relative
                display block
                font-size: 13px;
                width 100%
                i
