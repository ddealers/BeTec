@import "nib"
*
  margin 0
  padding 0
body
html
  size 100%
/* =========================== ESTILO GENERAL */
h1
  position absolute
  font-size 3.8vw
  color #fff
  transform rotate(-3deg)
  display block
  font-family 'HelveticaNeue-CondensedBold'
h2
  color #fff
  font-size 2.7vw
  font-family 'GillSans-Bold'
  font-weight bold
  margin-bottom 3vh
  @media screen and (max-width: 640px)
    font-size 1em
p
  color #fff
  font-size 1.2vw
  font-family 'GillSans-Light'
  font-weight regular
  @media screen and (max-width: 1024px)
    font-size 1.8vw
li
  font-family 'GillSans-Light'
a
  text-decoration none
/* =========================== NAV */
header
  fixed top left
  width 100%
  display inline-flex
  justify-content space-around
  align-items center
  z-index 3
  figure 
    img
      width 11vw
      min-width 135px
      position fixed
      left 93px
      top 15px
      @media screen and (max-width: 640px)
        min-width 70px
        left 0
        top 5px
  nav
    font-family 'Gill Sans'
    font-weight 300
    @media screen and (max-width: 1024px)
      padding-left 100px
    @media screen and (max-width: 640px)
      padding-left 0
    ul
      display inline-flex
      justify-content space-around
      align-items center
      list-style-type none
      li
        margin-right 3vw
        @media screen and (max-width: 640px)
          margin-right 0.5em
        a
          font-size 1.2vw
          color #fff
          &.selected
            font-weight 700
          @media screen and (max-width: 1024px)
            font-size 1.8vw 
          @media screen and (max-width: 640px)
            font-size 0.6em
  div.social
    display inline-flex
    flex-wrap wrap
    list-style none
    width 15vw
    margin-top 2vw
    @media screen and (max-width: 640px)
          position absolute
          right 0
          margin-top 2em
          margin-right 1em
          width 5em
    a
      width 100%
      display block
    div.rs
      width 5vw
      margin 0 auto
      @media screen and (max-width: 640px)
        width 2.5em
      a
        size 2vw
        text-align center
        display inline-block
        height 20px
        @media screen and (max-width: 640px)
          width 1em
  [class^="icon-"], [class*=" icon-"] 
    color: #fff;
nav.arrows
  fixed top 21vh right 11vw z-index 1
  @media screen and (max-width: 640px)
    display none
  a
    size 4vw
    display block
    float left
    &.prev
      background url(../img/flechas.png) no-repeat 
      background-size cover
    &.next
      background url(../img/flechas.png) no-repeat -4.5vw
      background-size cover
      margin-left: 2px

/* =========================== BE TEC */
section
  size 100%
  fixed top 0 left 0
  overflow hidden
  article
    absolute top 32vh left 16vw
    @media screen and (max-width: 1024px)
      top 31vh
    @media screen and (max-width: 640px)
      width 15em
      left 2em
      padding 0 1em
  &#be_tec
    background url(../img/filtro-rosa.png), url(../img/back-be-tec.jpg)no-repeat;
    background-size: cover
    h1
      top -6.6vw
      background #97ea3d
      padding 1vw 2.1vw
      left -7vw
      @media screen and (max-width: 1024px)
        top -7.9vw
      @media screen and (max-width: 640px)
        font-size 2em
        top -1.4em
    p
      margin-left 1.6vw
      @media screen and (max-width: 640px)
        font-size 0.7em
