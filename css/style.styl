                  min-width 9vw
                  border 2px #ececec solid
                  background #fff
                  color #727272
                  appearance none
                  line-height 135%
                  max-width 25vw
                  font-size 11px
                  @media screen and (max-width: 640px)
                    max-width 95%
              .bool-choose
              .bool-chooser
                .option
                  cursor pointer
                  size 2vw 1.5vw
                  color #ececec
                  background #fff
                  border 2px #ececec solid
                  padding 1vw
                  text-align center
                  display inline-block
                  @media screen and (max-width: 640px)
                    size 1.2em
            &.genero
              .choose
                border 1px solid #ececec 
                size 53vw 11vw
                position relative
                .option
                  cursor pointer
                  size 50% 100%
                  float left
                  text-align center
                  img
                    padding-top 10%
                    display block
                    margin auto
                    @media screen and (max-width: 640px)
                      padding 5%
                      width 15%
                  p
                    color #727272
                  &.selected
                    background #68b9ef
                    p
                      color #fff
                .o
                  absolute top right bottom left
                  size 3vw 1.5vw
                  margin auto
                  text-align center
                  background #f5f4f4
                  box-shadow 1px 1px 5px #ccc
                  @media screen and (max-width: 640px)
                    height 1.5em
            &.cumple
              margin-left 4vw
              .step-data
                margin-left 2vw
            &.contacto
              input[name="lada"]
                width 10%
                display inline-block
              input[name="tel"]
                width 70%
                display inline-block
            &.carreras
