/* =========================== REGISTRO */
  &#registro
    display none
    background #f9f9f9
    article
      top 11vw
      left 20vw
      size 65% 60%
      h1.registro
        top -4.6vw
        background #ff3073
        padding 1vw 2.1vw
        font-size 2.3vw 
        @media screen and (max-width: 640px)
          font-size 1.4em
          top -1.3em
      h2.experiencia 
        color #727272
        font-size 2.4vh
        margin 1vh 0
